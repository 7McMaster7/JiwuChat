{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile-capability",
  "windows": [
    "main",
    "msgbox"
  ],
  "platforms": ["iOS", "android"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:app:default",
    "core:event:default",
    "core:tray:default",
    "core:menu:default",
    "core:path:default",
    "core:webview:default",
    "core:image:default",
    "core:resources:default",
    "core:window:allow-close",
    "core:window:allow-toggle-maximize",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:window:allow-hide",
    "core:window:allow-destroy",
    "core:window:allow-is-enabled",
    "core:window:allow-is-fullscreen",
    "core:window:allow-is-closable",
    "core:window:allow-is-minimizable",
    "core:window:allow-is-maximizable",
    "core:window:allow-is-resizable",
    "core:window:allow-is-maximized",
    "core:window:allow-is-visible",
    "core:window:allow-get-all-windows",
    "core:window:allow-set-always-on-top",
    "core:window:allow-start-dragging",
    "core:window:allow-unmaximize",
    "core:window:allow-unminimize",
    "core:app:allow-version",
    "core:event:allow-emit",
    "core:event:allow-emit-to",
    "core:event:allow-listen",
    "core:event:allow-unlisten",
    "core:window:allow-set-always-on-bottom",
    "core:app:allow-set-app-theme",
    "os:default",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-hostname",
    "os:allow-version",
    "os:allow-locale",
    "os:allow-platform",
    "upload:default",
    "fs:default",
    "fs:read-all",
    "fs:allow-read",
    "fs:read-files",
    "fs:write-all",
    "fs:allow-resource-read-recursive",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-open",
    "shell:allow-stdin-write",
    "notification:default",
    "notification:allow-get-active",
    "notification:allow-is-permission-granted",
    "dialog:default"
  ]
}
